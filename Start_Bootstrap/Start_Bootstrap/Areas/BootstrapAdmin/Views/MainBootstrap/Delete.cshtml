@model MainBootstrap
@{
    ViewData["Title"] = "Delete";
}

<div>
    <div>
        <h4>
            Id
        </h4>
        <p>
            @Model.Id
        </p>
    </div>

    <div>
        <h4>Image</h4>
        <p>
            <img style="width:100px" src="~/assets/images/@Model.LogoImage"/>
        </p>
    </div>

    <div>
        <h4>Title</h4>
        <p>
            @Model.Title
        </p>
    </div>

    <div>
        <h4>Subtitle</h4>
        <p>
            @Model.SubTitle
        </p>
    </div>

    <div>
        <h4>Icon</h4>
        <p>
            <i style="font-size:25px;color:red" class="@Model.Icon"></i>
        </p>
    </div>

</div>

<form method="post" class="mt-4">
    <button type="submit" class="btn btn-danger">Delete</button>
</form>

