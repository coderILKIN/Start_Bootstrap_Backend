<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="~/assets/scss/css/script.css">
</head>
<body>
    <header>
        <div class="header-nav">
            <div class="container">
                <div class="row">
                    <div class="col-6 col-sm-6 col-md-6 col-lg-6">
                        <div class="logo">
                            <h1><a href="">START BOOTSTRAP</a></h1>
                        </div>
                    </div>
                    <div class="col-6 col-sm-6 col-md-6 col-lg-6">
                        <nav>
                            <ul class="d-flex justify-content-center">
                                <li><a href="">PORTFOLIO</a></li>
                                <li><a href="">ABOUT</a></li>
                                <li><a href="">REGISTER</a></li>
                                <li><a href="">LOGIN</a></li>
                            </ul>
                        </nav>

                        <div class="nav-icon d-flex justify-content-end">
                            <i class="fa-solid fa-bars"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>


    @RenderBody()


       <footer>
        <section id="bootstrap-footer">
            <div class="container">
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-4">
                            <div class="location mt-5">
                                <h4>LOCATION</h4>
                            </div>
                            <div class="adres mt-5">
                                <address>2215 John Daniel Drive
                                    Clark, MO 65243</address>
                            </div>
                    </div>
                    <div class="col-12 col-sm-12 col-md-12 col-lg-4">
                        <div class="around mt-5">
                            <h4>AROUND THE WEB</h4>
                        </div>
                        <div class="nav">
                            <ul class="d-flex">
                                <li><a href=""><i class="fa-brands fa-facebook"></i></a></li>
                                <li><a href=""><i class="fa-brands fa-twitter"></i></a></li>
                                <li><a href=""><i class="fa-brands fa-linkedin-in"></i></a></li>
                                <li><a href=""><i class="fa-solid fa-basketball"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-12 col-sm-12 col-md-12 col-lg-4">
                        <div class="freelancer mt-5">
                            <h4>ABOUT FREELANCER</h4>
                        </div>
                        <div class="freelancer-about mt-5">
                            <span>Freelance is a free to use, MIT licensed Bootstrap theme created by <a href="">Start Bootstrap</a>
                        </div>
                        
                    </div>
                </div>
            </div>
            <div class="copyright">
                <p>Copyright © Your Website 2022</p>
            </div>
        </section>
    </footer>
</body>
</html>